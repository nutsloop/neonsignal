  "compilerOptions": {
    "target": "ES2017",
    "module": "ESNext",
    "jsx": "preserve",
    "moduleResolution": "Node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": false,
    "noEmit": true
  },
  "include": [
    "neonjsx",
    "www/neonsignaljsx",
    "www/nutsloopjsx"
  ]
}
